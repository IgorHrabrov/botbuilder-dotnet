{
    "$schema": "https://raw.githubusercontent.com/microsoft/botbuilder-dotnet/chrimc/map/schemas/sdk.schema",
    "$type": "Microsoft.OnSetProperty",
    "property": "ActivityType",
    "actions": [
        {
            "$type": "Microsoft.IfCondition",
            "condition": "!turn.dialogevent.value.expected",
            "actions": [
                {
                    "$type": "Microsoft.IfCondition",
                    "condition": "!$ActivityType",
                    "actions": [
                        {
                            "$type": "Microsoft.SendActivity",
                            "activity": "[unexpectedPropertySet('ActivityType', @ActivityType, ActivityTypeName())]"
                        }
                    ],
                    "elseActions": [
                        {
                            "$type": "Microsoft.SendActivity",
                            "activity": "[unexpectedPropertyChange('ActivityType', @ActivityType, ActivityTypeName(), $ActivityType)]"
                        }
                    ]
                }
            ]
        },
        {
            "$type": "Microsoft.SetProperty",
            "property": "$ActivityType",
            "value": "@ActivityType"
        }
    ]
}