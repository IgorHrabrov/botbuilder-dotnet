{
    "$schema": "https://raw.githubusercontent.com/microsoft/botbuilder-dotnet/chrimc/map/schemas/sdk.schema",
    "$type": "Microsoft.OnSetProperty",
    "property": "Cheese",
    "actions": [
        {
            "$type": "Microsoft.SetProperty",
            "property": "$Cheese",
            "value": "@Cheese"
        },
        {
            "$type": "Microsoft.SetProperty",
            "property": "$PropertyOnhold",
            "value": "$Cheese"
        },
        {
            "$type": "Microsoft.IfCondition",
            "condition": "!turn.dialogevent.value.expected",
            "actions": [
                {
                    "$type": "Microsoft.Ask",
                    "activity": "[unexpectedPropertySet('Cheese', @Cheese, turn)]",
                    "expectedProperties": [
                        "SetPropertyConfirmation"
                    ]
                }
            ]
        }
    ]
}