
{
    "$schema": "https://raw.githubusercontent.com/microsoft/botbuilder-dotnet/chrimc/map/schemas/sdk.schema",
    "$type": "Microsoft.OnSetProperty",
    "property": "Cheese",
    "entity": "CheeseEntity",
    "actions": [
        {
            "$type": "Microsoft.IfCondition",
            "condition": "!turn.dialogevent.value.expected",
            "actions":[               
                {
                    "$type": "Microsoft.IfCondition",
                    "condition": "!$Cheese",
                    "actions":[
                        {
                            "$type": "Microsoft.SendActivity",
                            "activity": "@{unexpectedPropertySet('Cheese', @CheeseEntity)}"
                        }
                    ],
                    "elseActions":[
                        {
                            "$type": "Microsoft.SendActivity",
                            "activity": "@{unexpectedPropertyChange('Cheese', @CheeseEntity, $Cheese)}"
                        }
                    ]
                }                
            ]           
        },
        {
            "$type": "Microsoft.SetProperty",
            "property": "$Cheese",
            "value": "@CheeseEntity"
        }
    ]
}
